{snippet snippetBlock}
    <div class="nette-cookie-bar" n:if="!$showBlock">
        {_'cookie-bar-content'}
        <a n:href="HideBlock!" class="ajax nette-cookie-bar__close">{_'cookie-bar-button'}</a>
        <a href="https://www.google.com/policies/technologies/cookies/" target="_blank">{_'cookie-bar-link'}</a>
    </div>
    <noscript><style>.nette-cookie-bar { display: none; }</style></noscript>
{/snippet}
